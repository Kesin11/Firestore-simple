{
  "name": "firestore-simple",
  "version": "5.0.1",
  "description": "A simple wrapper for Firestore",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc --project .",
    "build:test": "tsc --project test",
    "test": "jest",
    "lint": "eslint src __tests__ --ext=.ts",
    "lint:fix": "npm run lint -- --fix",
    "coverage": "codecov",
    "publish:beta": "npm version prerelease && npm publish --tag beta",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "dependencies": {
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "codecov": "3.6.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "firebase-admin": "8.9.2",
    "jest": "24.9.0",
    "jest-circus": "24.9.0",
    "shipjs": "0.12.1",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/Firestore-simple.git"
  },
